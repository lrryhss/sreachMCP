{
  "name": "searxng-mcp-server",
  "version": "0.1.0",
  "description": "MCP server for SearXNG search integration",
  "mcpServers": {
    "searxng": {
      "command": "python",
      "args": [
        "src/server.py"
      ],
      "env": {
        "SEARXNG_BASE_URL": "http://localhost:8090",
        "MCP_TRANSPORT": "stdio",
        "LOG_LEVEL": "INFO"
      }
    }
  },
  "devDependencies": {
    "shadcn": "^3.3.1"
  }
}
